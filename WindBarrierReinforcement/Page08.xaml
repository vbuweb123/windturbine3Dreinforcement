                    <!--Bottom Offset-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_Int_BottomOffset" Content="__Bottom Offset" HorizontalAlignment="Left" Margin="5,35,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox                         
                        DataContext="{Binding DataModel_ARIntUBars}"
                        Text="{Binding BottomOffset, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="23" Margin="150,35,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>

                    <!--Axis Offset-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_Int_AxisOffset" Content="__Axis Offset" HorizontalAlignment="Left" Margin="5,65,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox                         
                        DataContext="{Binding DataModel_ARIntUBars}"
                        Text="{Binding AxisOffset, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,65,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>

                    <!--Diameter U Larger-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_Int_Diameter_Ularger" Content="__Diameter U Larger" HorizontalAlignment="Left" Margin="5,95,0,0" VerticalAlignment="Top" Height="25"/>
                    <ComboBox                        
                        DataContext="{Binding DataModel_ARIntUBars}"
                        ItemsSource="{Binding DiameterNames, Mode=OneWay}"
                        SelectedIndex="{Binding SelectedIndexDiameterL, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Margin="150,95,50,0" VerticalAlignment="Top" Width="150" Height="25"/>

                    <!--Diameter U Smaller-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_Int_Diameter_Usmaller" Content="__Diameter U Smaller" HorizontalAlignment="Left" Margin="5,125,0,0" VerticalAlignment="Top" Height="25"/>
                    <ComboBox                        
                        DataContext="{Binding DataModel_ARIntUBars}"
                        ItemsSource="{Binding DiameterNames, Mode=OneWay}"
                        SelectedIndex="{Binding SelectedIndexDiameterS, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Margin="150,125,50,0" Width="150" VerticalAlignment="Top" Height="25"/>

                    <!--Hook U Larger-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_Int_HookLength_Ularger" Content="__Hook U Larger" HorizontalAlignment="Left" Margin="5,155,0,0" VerticalAlignment="Top" Height="25"/>

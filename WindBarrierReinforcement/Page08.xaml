            <TabItem Header="Interior Circular Top" Margin="-3,-5,0,1">
                <Grid Background="{StaticResource ExpanderBackgroudBrush}" Margin="0,1,0,2">

                    <!--Diameter-->
                    <Label FontSize="12" Content="Diameter" HorizontalAlignment="Left" Margin="0,3,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.551,2.423" Height="26" Width="100"/>

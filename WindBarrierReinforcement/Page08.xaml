                    </TextBox>

                    <!--No Of Bars-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICL_NoOfBars" Content="__No Of Bars" HorizontalAlignment="Left" Margin="5,95,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox                         
                        x:Name="UI_TextBox_AR_ICL_NoOfBars"                       
                        DataContext="{Binding DataModel_ARIntCircularLateral}"
                        Text="{Binding NoOfBars, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Tag="{Binding SpacingEnabled}"
                        Template="{StaticResource AppTextBoxGeneric}" Style="{StaticResource TextBoxTagTrigger_EnabledIfDisabled}"
                        HorizontalAlignment="Left" Height="25" Margin="150,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour InitialValue="0"/>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <!--Length Bar Min-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICL_MinBarLength" Content="__Lenght Bar Min" HorizontalAlignment="Left" Margin="5,125,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox 
                        DataContext="{Binding DataModel_ARIntCircularLateral}"
                        Text="{Binding MinBarLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                        Template="{StaticResource AppTextBoxGeneric}" Width="150"
                        Height="25" Margin="150,125,50,0" TextWrapping="Wrap" VerticalAlignment="Top"/>

                    <!--Length Bar Max-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICL_MaxBarLength" Content="__Lenght Bar Max" HorizontalAlignment="Left" Margin="5,155,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox 
                        DataContext="{Binding DataModel_ARIntCircularLateral}"
                        Text="{Binding MaxBarLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                        Template="{StaticResource AppTextBoxGeneric}"
                        Height="25" Margin="150,155,50,0" TextWrapping="Wrap" Width="150" VerticalAlignment="Top"/>


                    <!--Top Extra Offset-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICL_TopExtraOffset" Content="__Top Extra Offset" HorizontalAlignment="Left" Margin="5,185,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox                                                 
                        DataContext="{Binding DataModel_ARIntCircularLateral}"
                        Text="{Binding TopExtraOffset, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}"
                        Height="25" Margin="150,185,50,0" Width="150" TextWrapping="Wrap" VerticalAlignment="Top"/>

                    <!--Bottom Extra Offset-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICL_BottomExtraOffset" Content="__Bottom Extra Offset" HorizontalAlignment="Left" Margin="5,215,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox                         
                        DataContext="{Binding DataModel_ARIntCircularLateral}"
                        Text="{Binding BottomExtraOffset, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                        Template="{StaticResource AppTextBoxGeneric}"
                        Height="25" Margin="150,215,50,0" TextWrapping="Wrap"  VerticalAlignment="Top" RenderTransformOrigin="-0.346,-1.87"/>
                </Grid>

            </TabItem>
            <TabItem Header="Interior Circular Bottom">
                <Grid Background="{StaticResource AppContainerBackgroundColor}" HorizontalAlignment="Stretch" Margin="0,0,0,0">
                    <!--Diameter-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICB_Diameter" Content="__Diameter" HorizontalAlignment="Left" Margin="5,5,0,0" VerticalAlignment="Top" Height="25"/>
                    <ComboBox                        
                        DataContext="{Binding DataModel_ARIntCircularBottom}"
                        ItemsSource="{Binding DiameterNames, Mode=OneWay}"
                        SelectedIndex="{Binding SelectedIndexDiameter, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Margin="150,5,50,0" Width="150" VerticalAlignment="Top" Height="25"/>

                    <!--Spacing-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICB_Spacing" Content="__Spacing" HorizontalAlignment="Left" Margin="5,35,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox 
                        DataContext="{Binding DataModel_ARIntCircularBottom}"
                        Text="{Binding Spacing, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}"
                        HorizontalAlignment="Left" Height="25" Margin="150,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>

                    <!--Length Bar Min-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICB_MinBarLength" Content="__Lenght Bar Min" HorizontalAlignment="Left" Margin="5,65,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox 
                        DataContext="{Binding DataModel_ARIntCircularBottom}"
                        Text="{Binding MinBarLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}"
                        HorizontalAlignment="Left" Height="25" Margin="150,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>

                    <!--Length Bar Max-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICB_MaxBarLength" Content="__Lenght Bar Max" HorizontalAlignment="Left" Margin="5,95,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox 
                        DataContext="{Binding DataModel_ARIntCircularBottom}"
                        Text="{Binding MaxBarLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}"
                        HorizontalAlignment="Left" Height="25" Margin="150,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>

                    <!--Start Extra Offset-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_ICB_StartExtraOffset" Content="__Start Extra Offset" HorizontalAlignment="Left" Margin="5,125,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox 
                        DataContext="{Binding DataModel_ARIntCircularBottom}"
                        Text="{Binding StartExtraOffset, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}"
                        HorizontalAlignment="Left" Height="25" Margin="150,125,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>

                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Page>

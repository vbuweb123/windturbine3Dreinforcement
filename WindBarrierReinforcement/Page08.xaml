                        DataContext="{Binding DataModel_ARIntCircularTop}"
                        Text="{Binding MinBarLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        HorizontalAlignment="Left" Height="23" Margin="124,68,0,0" TextWrapping="Wrap" 
                        VerticalAlignment="Top" Width="40" RenderTransformOrigin="-0.346,-1.87"/>

                    <!--Length Bar Max-->
                    <Label FontSize="12" Content="Lenght Bar Max" HorizontalAlignment="Left" Margin="0,101,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.551,2.423" Height="26" Width="123"/>
                    <TextBox 
                        x:Name="UI_TextBox_AR_ICT_MaxBarLength"    

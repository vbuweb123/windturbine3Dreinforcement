                    <Label ap:CultureKey.CultureRes="Lbl_AR_Int_HookLength_Usmaller" Content="__Hook U Smaller" HorizontalAlignment="Left" Margin="5,185,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox                         
                        DataContext="{Binding DataModel_ARIntUBars}"
                        Text="{Binding HookUSmaller, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,185,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>

                    <!--Top Extra Offset-->
                    <Label ap:CultureKey.CultureRes="Lbl_AR_Int_TopExtraOffset" Content="__Top Extra Offset" HorizontalAlignment="Left" Margin="5,215,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox                                                 
                        DataContext="{Binding DataModel_ARIntUBars}"
                        Text="{Binding TopExtraOffset, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,215,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>

                    <!--Bottom Extra Offset-->

                    <!--Length Bar Min-->
                    <Label FontSize="12" Content="Lenght Bar Min" HorizontalAlignment="Left" Margin="-1,66,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.551,2.423" Height="26" Width="111"/>

                HorizontalAlignment="Left" VerticalAlignment="Top" TextWrapping="Wrap" 
                Margin="140,5,0,0"  Height="25" Width="100">
                <i:Interaction.Behaviors>
                    <behaviors:NumericBehaviour InitialValue="0"></behaviors:NumericBehaviour>
                </i:Interaction.Behaviors>
            </TextBox>


                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <ComboBox
                        DataContext="{Binding DataModelRadial1}"
                        ItemsSource="{Binding DiameterNames, Mode=OneWay}"
                        SelectedIndex="{Binding SelectedIndexThirdDiameter, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Grid.Column="1" Margin="5,155,5,0" Height="24" Width="150" VerticalAlignment="Top"/>

                    <TextBox
                        DataContext="{Binding DataModelRadial1}"
                        Text="{Binding SpacingAngle, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"    

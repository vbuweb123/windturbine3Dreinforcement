                        Grid.Column="1" HorizontalAlignment="Left" Height="25" Margin="5,215,5,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                    </TextBox>

                    <TextBox
                        DataContext="{Binding DataModelRadial1}"
                        Text="{Binding LargeDiameterOffsetFromCenter, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"

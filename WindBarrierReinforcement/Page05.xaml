                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <TextBox
                        DataContext="{Binding DataModelRadial1}"
                        Text="{Binding Spacing3rdDiameterAngle, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        Grid.Column="1" HorizontalAlignment="Left" Height="25" Margin="5,455,5,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                    </TextBox>

                    <TextBox
                        DataContext="{Binding DataModelRadial1}"
                        Text="{Binding StartOffsetAngle, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        Grid.Column="1" HorizontalAlignment="Left" Height="25" Margin="5,485,5,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">

                    </TextBox>

                    <TextBox
                        DataContext="{Binding DataModelRadial1}"
                        Text="{Binding MaximumLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}"
                        Grid.Column="1" HorizontalAlignment="Left" Height="25" Margin="5,515,5,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>
                    <!--Grid Column 2 -->
                    <!--Grid Column 1 -->
                    <TextBox
                        DataContext="{Binding DataModelRadial2}"
                        Text="{Binding LargeDiamNoOfBars, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}"
                        Grid.Column="2" HorizontalAlignment="Left" Height="25" Margin="5,5,5,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <ComboBox
                        DataContext="{Binding DataModelRadial2}"
                        ItemsSource="{Binding DiameterNames, Mode=OneWay}"

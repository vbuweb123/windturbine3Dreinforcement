                    <TextBox
                        DataContext="{Binding DataModelRadial1}"
                        Text="{Binding LargeDiamNoOfBars, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}"
                        Grid.Column="1" HorizontalAlignment="Left" Height="25" Margin="5,5,5,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <ComboBox
                        DataContext="{Binding DataModelRadial1}"
                        ItemsSource="{Binding DiameterNames, Mode=OneWay}"
                        SelectedIndex="{Binding SelectedIndexLargeDiameter, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Grid.Column="1" Margin="5,35,5,0" Height="24" Width="150" VerticalAlignment="Top"/>

                    <TextBox
                        DataContext="{Binding DataModelRadial1}"
                        Text="{Binding SmallDiamNoOfBars, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}"
                        Grid.Column="1" HorizontalAlignment="Left" Height="25" Margin="5,65,5,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>

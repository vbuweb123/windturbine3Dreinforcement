<Page
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:WindBarrierReinforcement"
    xmlns:behaviors="clr-namespace:WindBarrierReinforcement.Behaviors"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" 
    xmlns:Behaviors="clr-namespace:WindBarrierReinforcement.Behaviors" 
    x:Class="WindBarrierReinforcement.Page05"
    mc:Ignorable="d" 
    xmlns:ap ="clr-namespace:WBR_Common.Common.AttachedProperties;assembly=WBR_Common"
    d:DesignHeight="580" d:DesignWidth="700"
    Title="Page05">

    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WBR_Common;component/Resources/XamlResources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>

    <Grid Name="UI_Grid_MasterGrid" Background="Transparent">
        <TextBlock ap:CultureKey.CultureRes="Tb_TopReinforcement" Text="__Top Reinforcement" HorizontalAlignment="Left" Margin="20,5,0,0"
                   TextWrapping="Wrap" VerticalAlignment="Top"
                   Style="{StaticResource  AppContentTitleTextBlock}" Name="UI_TextBlock_Title" />

        <TabControl TabStripPlacement="Left" HorizontalAlignment="Stretch" Margin="15,25,25,5" BorderThickness="0" Background="Transparent">
            <!--Tab Item Radial General-->
            <TabItem Header="Radial General">
                <Grid Background="{StaticResource AppContainerBackgroundColor}" HorizontalAlignment="Stretch" Margin="0,0,0,0">

                    <Label Content="__MinCore" ap:CultureKey.CultureRes="Lbl_TR_MinCore"
                           HorizontalAlignment="Left" Margin="5,5,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelRadialGeneral}"
                        Text="{Binding MinCore, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,5,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                    </TextBox>

                    <Label Content="__MinSpaceField" ap:CultureKey.CultureRes="Lbl_TR_MinSpaceField"
                           HorizontalAlignment="Left" Margin="5,35,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelRadialGeneral}"
                        Text="{Binding MinSpaceField, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}"
                        HorizontalAlignment="Left" Height="25" Margin="150,35,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <Label Content="__MinSpaceCenter" ap:CultureKey.CultureRes="Lbl_TR_MinSpaceCenter"
                           HorizontalAlignment="Left" Margin="5,65,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelRadialGeneral}"
                        Text="{Binding MinSpaceCenter, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}"
                        HorizontalAlignment="Left" Height="25" Margin="150,65,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <Label Content="__AlternativeMinStartAngle" ap:CultureKey.CultureRes="Lbl_TR_AlternativeMinStartAngle"
                           HorizontalAlignment="Left" Margin="5,95,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelRadialGeneral}"
                        Text="{Binding AlternativeMinStartAngle, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,95,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                    </TextBox>
                </Grid>
            </TabItem>
            <!--Tab Item Circular General-->
            <TabItem Header="Circular General">
                <Grid Background="{StaticResource AppContainerBackgroundColor}" HorizontalAlignment="Stretch" Margin="0,0,0,0">
                    <Label Content="__OVerlapLength" ap:CultureKey.CultureRes="Lbl_OverlapLength"
                           HorizontalAlignment="Left" Margin="5,5,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelCircularGeneral}"
                        Text="{Binding OverlapLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" 
                        HorizontalAlignment="Left" Height="25" Margin="150,5,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <Label Content="__MaximumRebarLength" ap:CultureKey.CultureRes="Lbl_TR_MaximumRebarLength"
                           HorizontalAlignment="Left" Margin="5,35,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelCircularGeneral}"
                        Text="{Binding MaximumRebarLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,35,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <Label Content="__DiameterEdgeCirculars" ap:CultureKey.CultureRes="Lbl_TR_DiameterEdgeCirculars"
                           HorizontalAlignment="Left" Margin="5,65,0,0" VerticalAlignment="Top" Height="25"/>
                    <ComboBox                        
                        DataContext="{Binding DataModelCircularGeneral}"
                        ItemsSource="{Binding DiameterNames, Mode=OneWay}" IsEnabled="False"
                        SelectedIndex="{Binding SelectedDiameterEdgeCirculars, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        Margin="150,65,4,0" VerticalAlignment="Top" Width="150" Height="25" HorizontalAlignment="Left"/>

                    <Label Content="__NumberEdgeCirculars" ap:CultureKey.CultureRes="Lbl_TR_NumberEdgeCirculars"
                           HorizontalAlignment="Left" Margin="5,95,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelCircularGeneral}"
                        Text="{Binding NumberEdgeCirculars, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,95,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <Label Content="__SpacingEdgeCirculars" ap:CultureKey.CultureRes="Lbl_TR_SpacingEdgeCirculars"
                           HorizontalAlignment="Left" Margin="5,125,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelCircularGeneral}"
                        Text="{Binding SpacingEdgeCirculars, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,125,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>

                    <Label Content="__RadiusCore" ap:CultureKey.CultureRes="Lbl_TR_RadiusCore"
                           HorizontalAlignment="Left" Margin="5,155,0,0" VerticalAlignment="Top" Height="25"/>
                    <TextBox
                        DataContext="{Binding DataModelCircularGeneral}"
                        Text="{Binding RadiusCore, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"    
                        Template="{StaticResource AppTextBoxGeneric}" IsEnabled="False"
                        HorizontalAlignment="Left" Height="25" Margin="150,155,50,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150">
                        <i:Interaction.Behaviors>
                            <behaviors:NumericBehaviour></behaviors:NumericBehaviour>
                        </i:Interaction.Behaviors>
                    </TextBox>
                </Grid>
            </TabItem>
